{
    "builds": [
      {
        "src": "bookmyseat/django_vercel.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bookmyseat/django_vercel.py"
      }
    ]
  }
  